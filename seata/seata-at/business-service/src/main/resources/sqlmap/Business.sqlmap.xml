<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.yiyi.seata.mapper.BusinessMapper">

    <insert id="add">
        insert into business_serial (account_no, amount) values (
            #{accountNo,jdbcType=VARCHAR},
            #{amount,jdbcType=DECIMAL}
        )
    </insert>

</mapper>